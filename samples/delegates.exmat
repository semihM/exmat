// String delegates
var str = "This is, as you can see, a STRING!"

printf("str.len() -> {0}\n", str.len())

printf("str.index_of(\"you\") -> {0}\n", str.index_of("you"))

printf("str.to_upper() -> {0}\n", str.to_upper())
printf("\tstr -> {0}\n", str)

printf("str.to_lower() -> {0}\n", str.to_lower())
printf("\tstr -> {0}\n", str)

printf("str.reverse() -> {0}\n", str.reverse())
printf("\tstr -> {0}\n", str)

printf("str.repeat(2) -> {0}\n", str.repeat(2))
printf("\tstr -> {0}\n", str)

printf("str.replace(\"you\",\"I\") -> {0}\n", str.replace("you","I"))
printf("\tstr -> {0}\n", str)

printf("str.slice(8) -> {0}\n", str.slice(8))

printf("str.slice(4,11) -> {0}\n", str.slice(4,11))

printf("str.isAlphabetic() -> {0}\n", str.isAlphabetic())
printf("str.isNumeric() -> {0}\n", str.isNumeric())
printf("str.isAlphaNumeric() -> {0}\n", str.isAlphaNumeric())
printf("str.isLower() -> {0}\n", str.isLower())
printf("str.isUpper() -> {0}\n", str.isUpper())
printf("str.isSymbol() -> {0}\n", str.isSymbol())

