// Dictionary delegates
var tbl =
{
    key1 = 123
    key2 = 555
    key3 = "testing"
    key4 = [-1, null, PI]
}

printf("tbl.len() -> {0}\n", tbl.len())

printf("tbl.get_keys() -> {0}\n", tbl.get_keys())

printf("tbl.get_values() -> {0}\n", tbl.get_values())

printf("tbl.has_key(\"key2\") -> {0}\n", tbl.has_key("key2"))
printf("\"key2\" in tbl -> {0}\n", "key2" in tbl)

printf("tbl.has_key(\"key999\") -> {0}\n", tbl.has_key("key999"))
printf("\"key999\" in tbl -> {0}\n", "key999" in tbl)

printf("tbl.random_key() -> {0}\n", tbl.random_key())

printf("tbl.random_val() -> {0}\n", tbl.random_val())
